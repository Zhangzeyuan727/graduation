eholder=\"请输入用户名\" v-model=\"user.name\"></mt-field>\n    <mt-field label=\"密码\" placeholder=\"请输入密码\" type=\"password\" v-model=\"user.pwd\"></mt-field>\n    <div class=\"loginBtn\" @click=\"goRegister\">注册</div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      user: {\n        name: \"\",\n        pwd: \"\",\n        uid: \"\"\n      }\n    };\n  },\n  methods: {\n    goRegister() {\n      this.$http\n        .userRegister({\n          name: this.user.name,\n          password: this.user.pwd\n        })\n        .then(res => {\n          if (res.data.statusCode == 1) {\n            //查询用户将用户信息保存至本地\n            this.$http\n              .userLogin({\n                name: this.user.name,\n                password: this.user.pwd\n              })\n              .then(res => {\n                if (res.data.statusCode == 1) {\n                  this.user.uid = res.data.results;\n                  //将用户信息保存至本地\n                  localStorage.setItem(\"userInfo\", JSON.stringify(this.user));\n                  this.$toast({\n                    message: \"注册成功\",\n                    duration: 3000\n                  });\n                  setTimeout(() => {\n                    this.$router.push({ path: this.$route.query.from });\n                  }, 3000);\n                }\n              });\n          }\n        });\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n#register {\n  position: absolute;\n  left: 0;\n  right: 0;\n  top: 0;\n  bottom: 0;\n  .registerTitle {\n    height: 10vh;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n  }\n  .loginBtn {\n    width: 80%;\n    height: 6vh;\n    margin-left: 10%;\n    margin-top: 20px;\n    border-radius: 10px;\n    text-align: center;\n    line-height: 6vh;\n    background-color: #bbd2c2;\n  }\n}\n</style>\n"]}]}